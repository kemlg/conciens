			<type>jar</type>
		</dependency>
		<dependency>
			<groupId>eu.superhub.wp3</groupId>
			<artifactId>situationaldata-model</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>clj-time</groupId>
			<artifactId>clj-time</artifactId>
